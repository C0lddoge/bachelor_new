units lj
dimension 3
atom_style atomic
boundary p p p
log log_equilibration.dat

read_data indata.in

pair_style star/cg 5.0
pair_coeff 1 1 166.0 1.0
pair_coeff 1 2 166.0 1.1
pair_coeff 1 3 166.0 0.9
 
pair_coeff 2 2 166.0 1.2
pair_coeff 2 3 166.0 1.0

pair_coeff 3 3 166.0 0.8 

fix mynvt all nvt temp 1.0 1.0 1.0


compute en all pe 
#fix myen all print 1000 "$en" file energy.txt 
thermo_style custom step pe
thermo 100


dump myltr all custom 100 dump.lammpstrj id type xs ys zs ix iy iz
dump_modify myltr sort id format line "%d %d %.5f %.5f %.5f %d %d %d"
timestep 0.01
run 1000000


write_restart restart.re
